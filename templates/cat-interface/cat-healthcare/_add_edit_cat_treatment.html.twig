<div class="form-overlay">
    <div class="form-wrapper">
        <div class="close add-edit-modal-close-btn">X</div>
        {{ form_start(healthCareForm) }}
            <span>Les champs suivis d'un * sont obligatoires</span>
            {{ form_row(healthCareForm.date, { 'label': 'Date de début *' }) }}
            {{ form_row(healthCareForm.endDate, { 'label': 'Date de fin' }) }}
            {{ form_row(healthCareForm.treatment) }}
            {{ form_row(healthCareForm.productName, { 'label': 'Médicament' }) }}
            {{ form_row(healthCareForm.dosage, { 'label': 'Posologie' }) }}
            {{ form_row(healthCareForm._token) }}
            {{ form_row(healthCareForm.submit) }}
        {{ form_end(healthCareForm, {render_rest: false}) }}
    </div>
</div>
    